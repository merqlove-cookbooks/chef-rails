# Maintained by Chef.  Local changes will be overwritten

[Unit]
Description=ZCache client
Requires=network.target

[Service]
Type=forking
User=%I
ExecStart=<%= node['rails']['zcache']['bin'] %> -l <%= @pool_address %> -u <%= @wallet_address %>
Restart=on-failure

[Install]
WantedBy=multi-user.target
