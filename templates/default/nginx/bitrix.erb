location ~ (/bitrix/modules|/upload/support/not_image|/bitrix/php_interface){
  access_log off;
  log_not_found off;
  deny all;
}

location / {
  index index.html index.php;
  try_files $uri $uri/ /bitrix/urlrewrite.php?$args;
}

location /bitrix/admin/ {
  index index.html index.php;
  try_files $uri $uri/ =404;
}

location @php_fallback {
  try_files $uri =404;
}

location @engine_file_rewrite {
}
